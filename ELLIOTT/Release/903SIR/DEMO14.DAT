at ptr file ssys1(iss1).900
j 9
st 6927

at ptr file mtbloc(iss4).900
j 9
st 6927

at ptr file mtrec(iss3).900
j 9
st 6927

at ptr inline
%
<! halt !><!!>
j 9
st 6927

c run program
nonstop on
j 32
st 284

sh t

c MLIST the tapes
reset
de mt 0
de mt 1

at mt 0 tape1
at mt 1 tape2

at ptr inline
*18
<! halt !><!!>
j 8
st 6927

at ptr file ../903utilities/mtlist(iss3).900
j 9
st 6927

at ptr file ../903sir/mtbloc(iss4).900
j 9
st 6927

at ptr file ../903sir/sglpak.900
j 9
st 6927

at ptr inline
%

[B1]
B1 0 ;+1
%
<! halt !><!!>
j 9
st 6927

c run mtlist
at tty inline
/ 0: TAPE1: L:) 
/ 1: TAPE2: L:)
<!!>
j 32
st 1575
j 32
st 1575

de mt 0
de mt 1
delete tape1
delete tape2